<reactiveui:ReactiveUserControl x:Class="Nymph.App.FunctionItemView" x:TypeArguments="itemViewModel:IFunctionItemViewModel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:reactiveui="http://reactiveui.net"
      xmlns:itemViewModel="clr-namespace:Nymph.Shared.ViewModel.ItemViewModel;assembly=Nymph.Shared"
      xmlns:local="clr-namespace:Nymph.App"
      mc:Ignorable="d"
      d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions >
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="Description"/>
    </Grid>
</reactiveui:ReactiveUserControl>
