<reactiveui:ReactiveUserControl x:Class="Nymph.Plugin.Everything.EverythingFileOpenItemView" x:TypeArguments="itemViewModel:ItemViewModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Nymph.Plugin.Everything"
             xmlns:reactiveui="http://reactiveui.net"
             xmlns:itemViewModel="clr-namespace:Nymph.Shared.ViewModel.ItemViewModel;assembly=Nymph.Shared"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="180"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Column="0" x:Name="FileNameBlock" Text="Unknown File"/>
    </Grid>
</reactiveui:ReactiveUserControl>
